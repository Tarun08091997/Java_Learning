import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String s = sc.nextLine();
        int n = s.length();
        String ns = "";
        sc.close();
        
        for(int i=n-1;i>=0;i--){
            ns = ns + s.charAt(i);
        }
        if(s.compareTo(ns)==0)
            System.out.println("Yes");
        else
            System.out.println("No");
    }
}
